{
  "name": "aswebglue",
  "version": "0.1.1",
  "ascMain": "src/WebGL.ts",
  "type": "module",
  "main": "src/ASWebGLue.js",
  "types": "src/ASWebGLue.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/battlelinegames/ASWebGLue"
  },
  "scripts": {
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
