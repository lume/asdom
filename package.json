    "imagemin": "^7.0.1",
    "imagemin-pngquant": "^9.0.1",
    "live-server": "^1.2.1",
    "make-dir-cli": "^2.0.0",
    "node-minify": "^3.6.0",
    "rimraf": "^3.0.2",
    "serve-static": "^1.14.1"
